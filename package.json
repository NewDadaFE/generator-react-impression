{
  "name": "generator-react-impression",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Yeoman generator for React project.",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json}'"
  },
  "dependencies": {
    "chalk": "~2.1.0",
    "mkdirp": "~0.5.1",
    "yeoman-generator": "~1.1.1",
    "yosay": "~2.0.1"
  },
  "devDependencies": {
    "husky": "~0.14.3",
    "lint-staged": "~4.1.3",
    "prettier": "~1.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier --write", "git add"]
  },
  "repository": "NewDadaFE/generator-react-impression",
  "engines": {
    "node": ">=6"
  },
  "files": ["app"],
  "keywords": [
    "react",
    "impression",
    "es6",
    "webpack",
    "scss",
    "yeoman-generator",
    "generator-react-impression"
  ]
}
