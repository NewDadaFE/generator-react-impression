{
  "name": "generator-react-impression",
  "version": "2.1.2",
  "license": "MIT",
  "description": "Yeoman generator for React project.",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier-eslint --write '**/*.{js,json}'"
  },
  "dependencies": {
    "chalk": "~2.1.0",
    "mkdirp": "~0.5.1",
    "ramda": "~0.24.1",
    "yeoman-generator": "~1.1.1",
    "yosay": "~2.0.1"
  },
  "devDependencies": {
    "eslint-config-react-impression": "~2.0.0",
    "husky": "~0.14.3",
    "lint-staged": "~4.1.3",
    "prettier-eslint-cli": "~4.3.2"
  },
  "eslintConfig": {
    "extends": "react-impression"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier-eslint --write", "git add"]
  },
  "repository": "NewDadaFE/generator-react-impression",
  "engines": {
    "node": ">=6"
  },
  "files": ["app"],
  "keywords": [
    "react",
    "impression",
    "es6",
    "webpack",
    "scss",
    "yeoman-generator",
    "generator-react-impression"
  ]
}
