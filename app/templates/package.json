{
  "name": "react-impression",
  "version": "0.1.0",
  "private": true,
  "description": "react-impression",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "start": "webpack-dev-server --env.development",
    "build": "webpack --env.production",
    "version": "npm run build && node deploy.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "~2.8.0",
    "babel-runtime": "^6.22.0",
    "dot-prop-immutable": "^1.3.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "qs": "~6.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-impression": "^1.2.5",
    "react-redux": "^5.0.4",
    "react-router": "3.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~7.1.6",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.2",
    "babel-loader": "~7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-imports": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "~0.28.7",
    "eslint": "~4.10.0",
    "eslint-config-react-impression": "~2.0.0",
    "eslint-loader": "~1.9.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "husky": "~0.14.3",
    "lint-staged": "~4.3.0",
    "node-dir": "^0.1.16",
    "node-sass": "~4.6.0",
    "postcss-loader": "~2.0.8",
    "postcss-scss": "~1.0.2",
    "prettier-eslint-cli": "~4.4.0",
    "qiniu": "^6.1.13",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.19.0",
    "url-loader": "~0.6.2",
    "webpack": "~3.8.1",
    "webpack-dev-server": "~2.9.4",
    "webpack-fn": "~0.1.1"
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "git add"]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel",
      [
        "transform-imports",
        {
          "lodash": {
            "transform": "lodash/${member}",
            "preventFullImport": true
          }
        }
      ],
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": "postcss-scss"
          },
          "webpackHotModuleReloading": true,
          "generateScopedName": "[name]__[local]___[hash:base64:5]"
        }
      ],
      "transform-runtime",
      "transform-object-rest-spread",
      "transform-class-properties",
      "transform-export-extensions"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"]
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "react-impression",
    "globals": {
      "__DEV__": false,
      "AMap": false
    }
  },
  "browserslist": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "proxy": {},
  "deploy": {
    "ACCESS_KEY": "",
    "SECRET_KEY": ""
  }
}
