{
  "name": "react-impression",
  "version": "0.1.0",
  "private": true,
  "description": "react-impression",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma all --write 'src/**/*.js'",
    "start": "NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "version": "npm run build && node config/deploy.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^2.7.0",
    "babel-runtime": "^6.22.0",
    "dot-prop-immutable": "^1.3.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "query-string": "^4.3.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-impression": "^1.2.5",
    "react-redux": "^5.0.4",
    "react-router": "3.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-imports": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-impression": "^1.0.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.2",
    "node-dir": "^0.1.16",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.5",
    "postcss-scss": "^1.0.0",
    "prettier": "^1.3.1",
    "qiniu": "^6.1.13",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "lint-staged": {
    "*.js": ["prettier --single-quote --trailing-comma all --write", "git add"]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel",
      [
        "transform-imports",
        {
          "lodash": {
            "transform": "lodash/${member}",
            "preventFullImport": true
          }
        }
      ],
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": "postcss-scss"
          },
          "webpackHotModuleReloading": true,
          "generateScopedName": "[name]__[local]___[hash:base64:5]"
        }
      ],
      "transform-runtime",
      "transform-object-rest-spread",
      "transform-class-properties",
      "transform-export-extensions"
    ],
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": ["impression", "prettier"],
    "globals": {
      "__DEV__": false,
      "AMap": false
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 1,
          "outerIIFEBody": 1
        }
      ],
      "react/jsx-indent": ["error", 2],
      "react/jsx-indent-props": ["error", 2],
      "newline-after-var": "off",
      "import/imports-first": "off"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "config/webpack.dev.js"
        }
      }
    }
  },
  "browserslist": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
