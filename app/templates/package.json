{
  "name": "react-impression",
  "version": "0.1.0",
  "private": true,
  "description": "react-impression",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "start": "webpack-dev-server --env.development",
    "debug": "webpack --env.debug && node deploy.js",
    "version": "webpack --env.production && node deploy.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "~3.3.2",
    "babel-runtime": "^6.22.0",
    "dot-prop-immutable": "^1.3.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "qs": "~6.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-impression": "^1.2.5",
    "react-redux": "^5.0.4",
    "react-router": "3.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.1.2",
    "babel-minify-webpack-plugin": "~0.2.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-imports": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "eslint": "~3.19.0",
    "eslint-config-react-impression": "~2.0.2",
    "eslint-config-standard": "~10.2.1",
    "eslint-config-standard-react": "~5.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-node": "~4.2.2",
    "eslint-plugin-promise": "~3.5.0",
    "eslint-plugin-react": "~6.10.0",
    "eslint-plugin-standard": "~3.0.1",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "husky": "~0.14.3",
    "json-loader": "^0.5.4",
    "lint-staged": "~5.0.0",
    "node-dir": "^0.1.16",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.5",
    "postcss-scss": "^1.0.0",
    "prettier-eslint-cli": "3.6.1",
    "qiniu": "^6.1.13",
    "react-hot-loader": "~3.1.3",
    "sass-loader": "^6.0.5",
    "style-loader": "~0.19.0",
    "url-loader": "~0.6.2",
    "webpack": "~3.9.1",
    "webpack-dev-server": "~2.9.7",
    "webpack-manifest-plugin": "~1.3.2",
    "webpack-merge": "~4.1.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel",
      [
        "transform-imports",
        {
          "lodash": {
            "transform": "lodash/${member}",
            "preventFullImport": true
          }
        }
      ],
      [
        "react-css-modules",
        {
          "exclude": "node_modules",
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          },
          "webpackHotModuleReloading": true,
          "generateScopedName": "[name]__[local]___[hash:base64:5]"
        }
      ],
      "transform-runtime",
      "transform-object-rest-spread",
      "transform-class-properties",
      "transform-export-extensions"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              ">1%",
              "last 4 versions",
              "Firefox ESR",
              "not ie < 9"
            ]
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "react-impression",
    "globals": {
      "DEBUG": false,
      "AMap": false
    }
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "proxy": {},
  "deploy": {
    "ACCESS_KEY": "",
    "SECRET_KEY": ""
  }
}
